  <insert id="updateCostMain">
  	insert into crm_cost_main
  (id, amt, cost_typeid, ORGANIZATION_ID, REGION_ID, PROV_ID)
  (SELECT seq_cost_main_id.nextval, 0, rec.*
     FROM (SELECT td.choose_val cost_type,
                  substr(cso.pid, 0, 5) orgid,
                  cso.pid,
                  cso.id
             FROM crm_sales_organization cso, t_dict td
            where cso.levels = '4'
              and td.col_name = 'COST_TYPE'
            order by cso.id, td.choose_val) rec
    where 1 = 1
      and not exists (SELECT 1
             FROM crm_cost_main ccm
            where 1 = 1
              and ccm.cost_typeid = rec.cost_type
              and ccm.organization_id = rec.orgid
              and ccm.region_id = rec.pid
              and ccm.prov_id = rec.id))
  </insert>
