package com.hhnz.account.model;

import java.math.BigDecimal;

import com.hhnz.util.BigDecimalASME;

public class CMerchCustAccountV {
    private String orgname;
    private String orgid;
    private Long merchCustId;
    private String custname;
    private BigDecimal cashAmt;
    private BigDecimal creditAmt;
    private BigDecimal subsidyAmt;
    private BigDecimal allamt;
    private BigDecimal bondAmt;
    private String pid;
    private String sapCustomerId;
    private String custType;
    private BigDecimal creditLines;
    private BigDecimal frozenAmt;
    private BigDecimal frozenSubsidy;
    private Long  id;
    private BigDecimal allocationAmt;
    private BigDecimal totalAmt;
    
	public BigDecimal getBondAmt() {
		return bondAmt;
	}
	public void setBondAmt(BigDecimal bondAmt) {
		this.bondAmt = BigDecimalASME.divide(bondAmt);
	}
	public String getOrgname() {
		return orgname;
	}
	public void setOrgname(String orgname) {
		this.orgname = orgname;
	}
	public String getOrgid() {
		return orgid;
	}
	public void setOrgid(String orgid) {
		this.orgid = orgid;
	}
	public Long getMerchCustId() {
		return merchCustId;
	}
	public void setMerchCustId(Long merchCustId) {
		this.merchCustId = merchCustId;
	}
	public String getCustname() {
		return custname;
	}
	public void setCustname(String custname) {
		this.custname = custname;
	}
	public BigDecimal getCashAmt() {
		return cashAmt;
	}
	public void setCashAmt(BigDecimal cashAmt) {
		this.cashAmt = BigDecimalASME.divide(cashAmt);
	}
	public BigDecimal getCreditAmt() {
		return creditAmt;
	}
	public void setCreditAmt(BigDecimal creditAmt) {
		this.creditAmt = BigDecimalASME.divide(creditAmt);
	}
	public BigDecimal getSubsidyAmt() {
		return subsidyAmt;
	}
	public void setSubsidyAmt(BigDecimal subsidyAmt) {
		this.subsidyAmt = BigDecimalASME.divide(subsidyAmt);
	}
	public BigDecimal getAllamt() {
		return allamt;
	}
	public void setAllamt(BigDecimal allamt) {
		this.allamt = BigDecimalASME.divide(allamt);
	}
	public String getPid() {
