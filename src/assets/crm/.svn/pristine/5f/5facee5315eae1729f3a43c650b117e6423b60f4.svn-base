	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	public String getSapCustomerId() {
		return sapCustomerId;
	}
	public void setSapCustomerId(String sapCustomerId) {
		this.sapCustomerId = sapCustomerId;
	}
	public String getCustType() {
		return custType;
	}
	public void setCustType(String custType) {
		this.custType = custType;
	}
	public BigDecimal getCreditLines() {
		return creditLines;
	}
	public void setCreditLines(BigDecimal creditLines) {
		this.creditLines = BigDecimalASME.divide(creditLines);
	}
	public BigDecimal getFrozenAmt() {
