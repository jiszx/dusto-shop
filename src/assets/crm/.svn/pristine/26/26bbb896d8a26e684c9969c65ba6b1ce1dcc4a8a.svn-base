package com.hhnz.account.dto;

import java.math.BigDecimal;

public class AccountReceiveDTO {
	private Long  upid;
	private BigDecimal payAmt;
	private BigDecimal receiveAmt;
	private BigDecimal unreceiveAmt;
	public Long getUpid() {
		return upid;
	}
	public void setUpid(Long upid) {
		this.upid = upid;
	}
	public BigDecimal getPayAmt() {
		return payAmt;
	}
	public void setPayAmt(BigDecimal payAmt) {
		this.payAmt = payAmt;
	}
	public BigDecimal getReceiveAmt() {
		return receiveAmt;
	}
	public void setReceiveAmt(BigDecimal receiveAmt) {
		this.receiveAmt = receiveAmt;
	}
	public BigDecimal getUnreceiveAmt() {
		return unreceiveAmt;
	}
	public void setUnreceiveAmt(BigDecimal unreceiveAmt) {
		this.unreceiveAmt = unreceiveAmt;
	}
	
}
