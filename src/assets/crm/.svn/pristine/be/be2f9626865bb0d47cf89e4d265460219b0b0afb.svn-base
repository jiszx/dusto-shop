package com.hhnz.customer.enu;

public enum CustPriceBatchMaintainDirection {
	NEWPRICE("1","重新定价"),
	RANGE("2","幅度调整");
	private String code;

	private String desc;
	private CustPriceBatchMaintainDirection(String code, String desc) {
		this.code = code;
		this.desc = desc;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	
}
