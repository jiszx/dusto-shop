---任务调用
create table t_schedule_job(
  jid number(12) PRIMARY key,
  job_name VARCHAR2(200) not null,
  job_type VARCHAR2(100) not null,
  job_desc VARCHAR2(500),
  cron varchar2(100),
  springId varchar2(100),
  method_name varchar2(200),
  status VARCHAR2(4) DEFAULT '0'
  create_ts DATE DEFAULT sysdate,
  update_ts DATE DEFAULT sysdate
);


create table t_task();

create table t_task_log();