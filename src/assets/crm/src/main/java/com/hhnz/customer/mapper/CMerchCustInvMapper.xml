  <select id="selectByDTO" resultMap="QueryResultMap" parameterType="com.hhnz.customer.dto.CustomerInvDTO">
  	select * from ( select rownum as rownum_,ci.*,cb.NAME as CUSTOMER_NAME,so.NAME as SALES_ORG_NAME, 
  		cb.SAP_CUSTOMER_ID, cb.CUST_TYPE, (select t.name from T_EMPLOYEE t WHERE t.id=cb.CREATE_OID ) as SALES_MAN, cb.STATES
		from C_MERCH_CUST_INV ci 
		left join C_MERCH_CUST_BASE cb on ci.MERCH_CUST_ID = cb.ID 
		<!-- left join T_MATERIAL_BASE mb on mb.sap_id = ci.MATERIAL_ID -->
		left join c_merch_cust_station cmcs on cmcs.merch_cust_id = cb.id
		left join CRM_SALES_ORGANIZATION so on substr(ci.ORGANIZATION_ID, 0, 5) = so.ID
		<where>
			<if test="inv.stationIds != null and inv.stationIds.size>0">
				and cmcs.station_id in
				 <foreach item="item" index="index" collection="inv.stationIds" open="(" separator="," close=")">  
		 		 #{item}  
		 		</foreach>
		 	</if>
	 		<if test="inv.customerName != null">
	 			and cb.NAME like '%' || #{inv.customerName} || '%'
	 		</if>
	 		<if test="inv.productName != null">
	 			and ci.ATTRIBUTE4 like '%' || #{inv.productName} || '%'
	 		</if>
	 		<if test="inv.date != null">
	 			and ci.CREATE_TS = #{inv.date}
	 		</if>
	 		<if test="inv.merchid != null">
	 			and ci.merch_cust_id = #{inv.merchid}
	 		</if>
				<![CDATA[and rownum <= #{end} order by ci.id desc]]>
	 	</where>
		) row_ 
		where row_.rownum_ >= #{begin}
  </select>
